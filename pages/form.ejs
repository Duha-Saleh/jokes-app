<section>
    <ul>
      <% arrray.foreach(element => {%>
        <li>
        <h2> Joke # <%= element.id%> </h2>
    <p>  <%= element.type%> </p>
    <p>  <%= element.setup%></p>
    <p> <%= element.punchline%> </p>
         <%- 'update'== formAction ? "update ": "add joke" %>
    let action =<% "/update/<%= element.id%>_?method="UPDATE" %>
    <form action="<%= action%>" method="POST">
    <label for="id"></label> Joke # <input type='number' name='id' value='<%= element.id%>'>
<label for="type">type</label><input type='text' name='type' value='<%= element.type%>'>
<label for="setup">setup</label><input type='text' name='setup' value='<%= element.setup%>'>
<label for="punchline">punchline</label><input type='text' name='punchline' value='<%= element.punchline%>'>

    <button type="submit"><%- 'update'== formAction ? "Update ": "add joke" %> </button>
    </form>
    <% })%>

    </li>
    </ul>
    </section>
    
    