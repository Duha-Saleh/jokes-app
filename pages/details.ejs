<section>
    <ul>
      <% arrray.foreach(element => {%>
        <li>
        <h2> Joke # <%= element.id%> </h2>
    <p>  <%= element.type%> </p>
    <p>  <%= element.setup%></p>
    <p> <%= element.punchline%> </p>
    
    <form action="/update/<%= element.id%>?_method="DELETE"" method="POST">
    <button type="submit"> DELETE </button>
    </form>
    <form action="/edit/<%= element.id%>" method="GET">
        <button type="submit"> UPDATE </button>
        </form>
    
    <% })%>
    
    </li>
    </ul>
    </section>
    
    